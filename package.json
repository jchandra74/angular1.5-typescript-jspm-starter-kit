{
  "name": "angular1.5-typescript-jspm-starter",
  "version": "1.0.0",
  "description": "Angular 1.5 Component with TypeScript and JSPM starter kit",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "jspm install && typings install",
    "jspm": "jspm",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "bundle:prod": "jspm bundle app --minify --inject",
    "bundle": "jspm bundle app --inject"
  },
  "keywords": [
    "angular",
    "1.5",
    "component",
    "typescript",
    "jspm",
    "rxjs"
  ],
  "author": "Jimmy Chandra",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "concurrently": "^3.1.0",
    "jspm": "^0.16.48",
    "lite-server": "^2.2.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.8",
    "typings": "^2.0.0"
  },
  "jspm": {
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.5",
      "rxjs": "npm:rxjs@^5.0.0-rc.2"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "rxjs": "^5.0.0-rc.2"
  }
}
